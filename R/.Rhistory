if (grepl(".zip", filename) | grepl(".htm", filename)) {
stop("Do not provide file extension in `filename`.")
}
# path to the dataset
path_to_data <- path
# append the path with the zip file
tmp <- paste0(path_to_data, zipfile)
# specify the specific file to load and load it
filename.data <- paste0(filename, ".DAT")
data <- read.table(unz(tmp, filename.data),
fill = TRUE)
# load column names
filename.meta <- paste0(filename, ".htm")
metadata <- read.table(unz(tmp, filename.meta),
fill = TRUE)
return(list(filename.data = filename.data, filename.meta = filename.meta))
}
